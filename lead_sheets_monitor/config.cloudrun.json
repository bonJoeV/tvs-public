{
  "settings": {
    "default_spreadsheet_id": "ENV:DEFAULT_SPREADSHEET_ID",
    "log_retention_days": 7,
    "api_timeout_seconds": 15,
    "retry_max_attempts": 2,
    "retry_base_delay_seconds": 1.0,
    "rate_limit_delay_seconds": 10.0,
    "smtp": {
      "host": "smtp.gmail.com",
      "port": 587,
      "username": "ENV:SMTP_USERNAME",
      "from_address": "ENV:SMTP_FROM_ADDRESS"
    },
    "email": {
      "admin_recipients": [
        "ENV:ADMIN_EMAIL"
      ]
    },
    "dlq_enabled": true,
    "dlq_max_retry_attempts": 12,
    "log_level": "DEBUG"
  }
}
